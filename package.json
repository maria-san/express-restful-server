    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.5",
        "@babel/preset-env": "^7.1.5",
        "babel-eslint": "^10.0.1",
        "babel-plugin-dynamic-import-node": "^2.2.0",
        "rimraf": "^2.6.2",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.16.0"
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "express": "^4.16.4"
    }
