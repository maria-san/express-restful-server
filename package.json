{
    "name": "express-restful-server",
    "version": "1.0.0",
    "description": "Express RESTful server",
    "main": "index.js",
    "scripts": {
        "build": "rimraf ./build && babel ./src -d ./build --copy-files",
        "serve": "node ./build/index.js",
        "start": "npm run build && npm run serve",
